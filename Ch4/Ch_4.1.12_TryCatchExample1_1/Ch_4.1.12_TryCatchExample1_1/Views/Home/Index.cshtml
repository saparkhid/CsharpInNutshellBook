@model List<int>
@{
    ViewBag.Title = "Home Page";

}

<main>



    <div class="text-center">
        <h1 class="display-4">Try catch example 1 </h1>
        @{
            if (ViewBag.ErrorMessage != null)
            {
                <h3 style="text-align:left; color:chocolate;"> @ViewBag.ErrorMessage </h3>
            }
            else
            {
                for (int i = 0; i < Model.Count; i++)
                {
                    <h3 style="text-align:left; color:chocolate;">@Model[i].ToString()</h3>
                    <hr />
                }

            }
        }
     


    </div>

</main>